{
  "name": "ssr-react",
  "version": "1.0.0",
  "description": "ssr-react",
  "keywords": [
    "boilerplate",
    "ssr",
    "react"
  ],
  "author": "tylee",
  "main": "app-shell.html",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/7ylee/ssr-react.git"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "webpack:dev": "rm -rf ./dev && webpack -w --mode development --config webpack.config.js",
    "production": "NODE_ENV=production node scripts/start.js",
    "development": "npm run webpack:dev & NODE_ENV=development babel-node scripts/start.js",
    "build": "rm -rf ./build && webpack --config webpack.prod.js",
    "docker:build": "docker build --rm -t tylee/ssr-react .",
    "docker:start": "docker run --rm -it -p 8080:80 tylee/ssr-react",
    "docker": "npm run docker:build && npm run docker:start"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "axios": "^0.18.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "body-parser": "^1.19.0",
    "chokidar": "^3.3.0",
    "compression-webpack-plugin": "^2.0.0",
    "core-js": "^3.4.5",
    "dotenv-webpack": "^1.7.0",
    "dotenv": "^7.0.0",
    "eslint-config-7ylee": "0.0.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint": "^6.7.2",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "html-webpack-change-assets-extension-plugin": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.11",
    "offline-plugin": "^5.0.6",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react": "^16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "redux": "^4.0.1",
    "serialize-javascript": "^2.1.1",
    "styled-components": "^4.2.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-pwa-manifest": "^4.0.0",
    "webpack": "^4.30.0",
    "winston": "^3.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      [
        "styled-components",
        {
          "displayName": false,
          "ssr": true,
          "minify": true,
          "pure": true
        }
      ]
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE >= 9"
  ]
}