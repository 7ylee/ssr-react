{
  "name": "ssr-react",
  "version": "1.0.0",
  "description": "ssr-react",
  "keywords": [
    "boilerplate",
    "ssr",
    "react"
  ],
  "author": "tylee",
  "main": "app-shell.html",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/7ylee/ssr-react.git"
  },
  "scripts": {
    "lint": "eslint src",
    "dev:client": "rm -rf ./dev && webpack --config webpack.config.js && node ./dev/server.js",
    "dev:server": "webpack -w --mode none --config webpack.config.js & nodemon ./dev/server.js",
    "build": "rm -rf ./dist && webpack --config webpack.prod.js",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "express": "^4.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.6.1",
    "styled-components": "^4.1.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "^5.15.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.10",
    "offline-plugin": "^5.0.6",
    "prettier": "^1.16.4",
    "redux-devtools-extension": "^2.13.8",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.7.2",
    "webpack-pwa-manifest": "^4.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      [
        "styled-components",
        {
          "displayName": false,
          "ssr": true,
          "minify": true,
          "pure": true
        }
      ]
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE >= 9"
  ]
}
